<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/></startup>
    <appSettings>
      <!-- Add any truck IDs (aka folder names) that are added as mods. Trucks from the base game will be included automatically. Separate trucks using the pipe character ( | ) -->
      <add key="TruckIds" value=""/>
      
      <!-- Add your DDS file names here, EXCLUDING both flag_ and .dds!  Separate flag names using the pipe character ( | ) -->
      <add key="FlagNames" value="cle"/>

      <!-- Add the user-friendly name for each flag, in the same order as the dds files entered above. -->
      <!-- This is the name displayed to the user. Separate names using the pipe character ( | ) -->
      <add key="FriendlyFlagNames" value="Cleveland Indians"/>
      
      <!-- The output directory for the generated files. Do NOT include a trailing slash! -->
      <add key="SaveLocation" value="C:\SCSFlagExport"/>
    </appSettings>
</configuration>
